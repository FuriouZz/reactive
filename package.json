{
  "name": "@furiouzz/reactive",
  "version": "0.0.6-beta.7",
  "type": "module",
  "main": "./esm/index.js",
  "types": "./types/index.d.ts",
  "typings": "./types",
  "description": "",
  "scripts": {
    "start": "tsc -p tsconfig.json -w",
    "build": "rm -rf dist && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "bump": "node bump.mjs",
    "deploy": "node deploy.mjs",
    "test": "jest --rootDir ./"
  },
  "author": "Christophe MASSOLIN <frenchise93@gmail.com> (https://chrsmsln.com)",
  "license": "Apache Licence 2.0",
  "dependencies": {
    "@furiouzz/lol": "^0.0.36-beta.6",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-jest": "^28.0.3",
    "jest": "^28.0.3",
    "semver": "^7.3.7"
  },
  "exports": {
    ".": {
      "node": {
        "require": "./cjs/index.js",
        "import": "./esm/index.js",
        "types": "./types/index.d.ts"
      },
      "browser": {
        "require": "./cjs/index.js",
        "import": "./esm/index.js",
        "types": "./types/index.d.ts"
      }
    },
    "./*": {
      "node": {
        "require": "./cjs/*",
        "import": "./esm/*",
        "types": "./types/*"
      },
      "browser": {
        "require": "./cjs/*",
        "import": "./esm/*",
        "types": "./types/*"
      }
    }
  }
}
