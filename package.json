{
  "name": "@furiouzz/reactive",
  "version": "0.0.6-beta.2",
  "type": "commonjs",
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "dist",
  "description": "",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "tsc -p tsconfig.json -w",
    "build": "rm -rf dist && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "bump": "node bump.js",
    "publish": "node publish.js",
    "test": "jest --rootDir ./"
  },
  "author": "Christophe MASSOLIN <frenchise93@gmail.com> (https://chrsmsln.com)",
  "license": "Apache Licence 2.0",
  "dependencies": {
    "@furiouzz/lol": "^0.0.36-beta.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-jest": "^28.0.3",
    "jest": "^28.0.3",
    "semver": "^7.3.7"
  },
  "exports": {
    ".": {
      "node": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "browser": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/cjs/index.js",
        "types": "./dist/types/index.d.ts"
      }
    },
    "./*": {
      "node": {
        "require": "./dist/cjs/*",
        "import": "./dist/esm/*",
        "types": "./dist/types/*"
      },
      "browser": {
        "require": "./dist/cjs/*",
        "import": "./dist/cjs/*",
        "types": "./dist/types/*"
      }
    }
  }
}
